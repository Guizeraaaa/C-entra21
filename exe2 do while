using System;

Console.WriteLine("Hello, World!");
int num;
int gerNum = new Random().Next(1,100);

do
{
    Console.WriteLine("Digite Novamente");
    num = Convert.ToInt32(Console.ReadLine());
} while (gerNum != num);

Console.WriteLine($"O n√∫mero escolhido foi {num}");
