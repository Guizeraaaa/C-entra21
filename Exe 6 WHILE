// See https://aka.ms/new-console-template for more information
Console.WriteLine("Hello, World!");


Console.WriteLine("Informe um número inteiro a ser multiplicado");
int numMult = Convert.ToInt32(Console.ReadLine());

int numCont=10;
 int numRecebe = 0;

while (numCont >= 1 && numCont <= 10)
{
   Console.WriteLine (numRecebe =  numMult * numCont);
    Console.WriteLine($"Múltiplo {numCont}: {numRecebe}");
   numCont--;

}
