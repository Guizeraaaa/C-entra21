// See https://aka.ms/new-console-template for more information
//Console.WriteLine("Hello, World!");

double varseq, resultSoma = 0;

Console.WriteLine("Insira uma sequência de números positivos. Insira um número negativo para parar.");

while (true)
{

    varseq = Convert.ToDouble(Console.ReadLine());

    if (varseq < 0) { break; } if (varseq >= 0)
    {
        resultSoma += varseq;
    }

}

Console.WriteLine($"A soma dos números positivos inseridos é: {resultSoma}");
